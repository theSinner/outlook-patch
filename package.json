    "antd": "^5.16.0",
    "date-fns": "^3.6.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@microsoft/microsoft-graph-types": "^2.40.0",
